<%= simple_form_for(@audio) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>

  <%= f.input :file, as: :file %>

  <%= f.association :students, input_html: { multiple: true }  %>
    </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>

<% end %>
